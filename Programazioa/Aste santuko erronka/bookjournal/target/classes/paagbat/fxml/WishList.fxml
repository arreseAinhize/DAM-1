<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="wishbackground" prefHeight="600" prefWidth="425.0" stylesheets="@../css/modena.css"
   xmlns="http://javafx.com/javafx/23.0.1"
   xmlns:fx="http://javafx.com/fxml/1" fx:controller="paagbat.controller.WishListController">
   <HBox layoutX="30.0" layoutY="504.0" prefHeight="26.0" prefWidth="324.0">
      <children>
         <HBox alignment="CENTER" prefHeight="26.0" prefWidth="110.0">
            <children>
               <Button alignment="CENTER" onAction="#handleNew" style="-fx-background-color: #6e94b8; -fx-text-fill: white;" text="Berria" textFill="#004aad" />
            </children>
         </HBox>
         <HBox alignment="CENTER" prefHeight="26.0" prefWidth="110.0">
            <children>
               <Button onAction="#handleBuy" style="-fx-background-color: #6e94b8; -fx-text-fill: white;" text="Erosita" />
            </children>
         </HBox>
         <HBox alignment="CENTER" prefHeight="26.0" prefWidth="110.0">
            <children>
               <Button onAction="#handleDelete" style="-fx-background-color: #6e94b8; -fx-text-fill: white;" text="Ezabatu" />
            </children>
         </HBox>
      </children>
   </HBox>
   <Button alignment="CENTER" layoutX="382.0" layoutY="7.0" onAction="#handleLogOut" prefHeight="34.0" prefWidth="24.0" style="-fx-background-color: #668094; -fx-text-fill: white;" textFill="#004aad">
      <graphic>
         <ImageView fitHeight="21.0" fitWidth="18.0">
            <image>
               <Image url="@../img/icon/home.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button layoutX="382.0" layoutY="173.0" onAction="#handleStats" prefHeight="33.0" prefWidth="34.0" style="-fx-background-color: #668094;" styleClass="icon-button">
      <graphic>
         <ImageView fitHeight="25.0" fitWidth="24.0">
            <image>
               <Image url="@../img/icon/stats.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button layoutX="382.0" layoutY="49.0" onAction="#handleRating" prefHeight="33.0" prefWidth="34.0" style="-fx-background-color: #668094;" styleClass="icon-button">
      <graphic>
         <ImageView fitHeight="25.0" fitWidth="24.0">
            <image>
               <Image url="@../img/icon/rating.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button layoutX="382.0" layoutY="92.0" onAction="#handleLog" prefHeight="33.0" prefWidth="34.0" style="-fx-background-color: #668094;" styleClass="icon-button">
      <graphic>
         <ImageView fitHeight="25.0" fitWidth="24.0">
            <image>
               <Image url="@../img/icon/info.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button layoutX="382.0" layoutY="132.0" onAction="#handleWish" prefHeight="33.0" prefWidth="34.0" style="-fx-background-color: #668094;" styleClass="icon-button">
      <graphic>
         <ImageView fitHeight="25.0" fitWidth="24.0">
            <image>
               <Image url="@../img/icon/wishlist.png" />
            </image>
         </ImageView>
      </graphic>
   </Button>
   <TableView fx:id="wishList_TableView" layoutX="23.0" layoutY="136.0" prefHeight="356.0" prefWidth="338.0">
      <columns>
      </columns>
   </TableView>
</AnchorPane>
