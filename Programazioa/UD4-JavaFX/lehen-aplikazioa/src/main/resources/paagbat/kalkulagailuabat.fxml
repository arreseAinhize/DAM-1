<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171"
    xmlns:fx="http://javafx.com/fxml/1" fx:controller="paagbat.KalkulagailuaBatController">

    <children>
        <!-- Segundo grupo -->
        <Label text="ZENBAKI BIREN ARTEKO ERAGIKETA GAUZATZEKO SAKATU '=' BOTOIA" />
        <TextField fx:id="lehenengoZenbakia" text="" />
        <Button fx:id="eragiketaMota" text="+" />
        <TextField fx:id="bigarrenZenbakia" text="" />
        <Button fx:id="eragiketa" text="=" onAction="#eragiketa" />

        <!-- Tercer grupo -->
        <Label text="Select View" />
        <Button fx:id="secondaryButton" text="Switch to Primary View" onAction="#switchToPrimary" />
        <Button fx:id="primaryButton" text="Switch to Secondary View" onAction="#switchToSecondary" />
        <Button fx:id="eragiketakButton" text="Switch to Eragiketak View" onAction="#switchToEragiketak" />
    </children>

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>