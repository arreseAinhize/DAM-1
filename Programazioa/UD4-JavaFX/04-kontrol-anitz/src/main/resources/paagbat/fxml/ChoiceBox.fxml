<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="paagbat.controller.ChoiceBoxController">
   <children>
      <Label id="lbIzenburua" text="ChoiceBox (ak eta ComboBoxak lantzeko eszena)" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Nongoa zara? (ChoiceBox)" />
            <ChoiceBox fx:id="choiceBoxHerriak" prefWidth="150.0" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="Non biziko zinateke gustora? (ComboBox)" />
            <ComboBox fx:id="comboBoxHerriak" editable="true" prefWidth="150.0" />
         </children>
      </HBox>
      <Button fx:id="btnAtzera" mnemonicParsing="false" onAction="#handleAtzera" prefHeight="24.0" prefWidth="300.0" text="ATZERA" />
   </children>
</VBox>
